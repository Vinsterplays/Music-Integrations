{
	"geode": "5.0.0-alpha.2",
	"gd": {
		"win": "2.2081",
		"mac": "2.2081"
	},
	"id": "vinsterplays.music_integrations",
	"name": "Music Integrations",
	"version": "v2.0.0-alpha.2",
	"developer": "Vinsterplays",
	"description": "Control YT/Spotify without tabbing out.",
	"repository": "https://github.com/Vinsterplays/Music-Integrations",
	"tags": ["music", "enhancement", "editor", "universal"],
	"resources": {
		"sprites": ["resources/*.png"],
		"files": [
			"resources/*.txt",
			"resources/BitmapFonts/*.fnt",
			"resources/BitmapFonts/*.png",
			"resources/*.png",
			"resources/foxy/*.png",
			"resources/foxy/*.ogg"
		]
	},
	"settings": {
		"key_overlay": {
			"type": "custom:keybind",
			"name": "Quick Overlay Keybind",
			"description": "Press this key to show the quick options overlay",
			"default": 164
		},
		"overlay_anchor": {
			"type": "string",
			"name": "Overlay Anchor",
			"description": "The corner of the screen the overlay should be anchored to.",
			"default": "Bottom Right",
			"one-of": ["Top Left", "Top Right", "Bottom Left", "Bottom Right", "Middle Left", "Middle Right"]
		}
	}
}
